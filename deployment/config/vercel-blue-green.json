{
  "version": 2,
  "builds": [
    {
      "src": "packages/web/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "packages/web/$1"
    }
  ],
  "functions": {
    "packages/web/api/**/*.ts": {
      "runtime": "@vercel/node"
    }
  },
  "env": {
    "BLUE_GREEN_ACTIVE": "blue",
    "CANARY_PERCENTAGE": "10"
  },
  "regions": ["iad1"],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}